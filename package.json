{
  "name": "TunesDeck",
  "version": "1.0.0",
  "description": "Music feed in real time.",
  "scripts": {
    "start": "./node_modules/cross-env/bin/cross-env.js NODE_ENV=development node server.dev.js",
    "serve": "./node_modules/cross-env/bin/cross-env.js NODE_ENV=production node server.prod.js",
    "build": "./node_modules/cross-env/bin/cross-env.js NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.prod.config.js --progress --colors -p",
    "deploy": "./node_modules/surge/lib/cli.js ./dist"
  },
  "author": "Siddhartth Jain",
  "license": "ISC",
  "dependencies": {
    "re-base": "^4.0.0",
    "react": "^16.4.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-flip-move": "^3.0.2",
    "react-timeago": "^4.1.9"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.0",
    "postcss-focus": "^3.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-reporter": "^5.0.0",
    "postcss-simple-vars": "^4.1.0",
    "react-motion": "^0.5.2",
    "style-loader": "^0.21.0",
    "surge": "^0.20.1",
    "webpack": "^4.16.4",
    "webpack-dev-server": "^3.1.5"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  }
}
